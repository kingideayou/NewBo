<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/style_color_primary</item>
        <item name="android:colorPrimaryDark"
            tools:ignore="NewApi">@color/style_color_primary_dark</item>
        <!-- <item name="android:windowBackground">@color/background_floating_material_light</item> -->
    </style>

    <style name="listViewMultiImageView">
        <item name="android:scaleType">centerCrop</item>
        <item name="android:layout_height">90dp</item>
        <item name="android:layout_width">90dp</item>
        <item name="android:layout_margin">2.5dp</item>
    </style>

    <style name="ContextMenuButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/btn_context_menu</item>
    </style>

</resources>
